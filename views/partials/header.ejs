      padding: 40px 0;
      text-align: center;
    }

    footer a {
      color: #79acff;
      transition: color 0.3s ease;
    }

    footer a:hover {
      color: #53ff7e;
    }

    .footer-icons {
      font-size: 2rem;
      margin: 0 15px;
    }

    .back-to-top {
      position: absolute;
      right: 20px;
      top: 20px;
    }

    .album {
      background-color: rgb(24, 45, 52);
    }

    .card {
      background-color: #f8f9fa;
      transition: transform 0.3s ease;
    }

    .card:hover {
      transform: translateY(-5px);
      box-shadow: 0 0 15px rgba(255, 255, 255, 0.2);
    }

    .card-title {
      color: #182C39;
    }

    .card-text {
      color: #555;
    }

    .section-title {
      color: #ffffff;
    }

    hr {
      border-color: #ffffff;
      opacity: 0.5;
    }

    .card-img-top {
      height: 300px !important;
    }

    .card:hover {
      transform: translateY(-5px);
      box-shadow: 0 0 20px rgba(0, 0, 0, 0.15);
      transition: all 0.3s ease-in-out;
    }

    .card-img-top {
      height: 220px;
      object-fit: fill;
      border-top-left-radius: 0.5rem;
      border-top-right-radius: 0.5rem;
    }

    .card-title {
      font-size: 1.25rem;
      font-weight: bold;
      color: #333;
    }

    .card-text {
      color: #666;
      font-size: 0.95rem;
    }

    .btn i:hover {
      transform: scale(1.2);
      transition: transform 0.2s;
    }

    .btn-sm {
      padding: 0.3rem 0.5rem;
    }

    .footer-icons i:hover {
      color: #ffc107;
      transform: scale(1.2);
      transition: 0.3s;
    }

    .hover-up:hover {
      transform: translateY(-5px);
      color: #ffc107;
      transition: 0.3s ease;
    }

    footer ul li a:hover {
      color: #ffc107;
      transition: 0.3s;
    }

    .btn-sm.text-primary {
      color: rgba(45, 97, 122, 0.732) !important;
    }

    .btn-sm.text-info {
      color: rgba(36, 194, 102, 0.841) !important;
    }

    .btn-sm.text-warning {
